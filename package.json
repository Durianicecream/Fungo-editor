{
	"name": "fungo-editor",
	"version": "0.1.0",
	"description": "一个基于slate.js的所见即所得的富文本编辑器",
	"author": "TB",
	"license": "MIT",
	"keywords": ["rich-editor", "slate"],
	"repository": "https://github.com/Durianicecream/Fungo-editor",
	"scripts": {
		"start":
			"webpack-dev-server --mode development --history-api-fallback --hot --inline --progress --colors",
		"build":
			"rm -rf dist/ && webpack --mode production --config webpack.config.prod.js --progress --colors"
	},
	"dependencies": {
		"antd": "^3.4.5",
		"immutable": "^3.8.2",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"slate": "0.33.4",
		"slate-html-serializer": "^0.6.2",
		"slate-react": "0.12.4"
	},
	"devDependencies": {
		"autoprefixer": "^8.4.1",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-import": "^1.7.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-runtime": "6.26.0",
		"css-loader": "^0.28.11",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.0.0",
		"less": "^3.0.2",
		"less-loader": "^4.1.0",
		"postcss-loader": "^2.1.4",
		"prettier": "^1.12.1",
		"style-loader": "^0.19.1",
		"url-loader": "^0.6.2",
		"webpack": "^4.6.0",
		"webpack-bundle-analyzer": "^2.11.1",
		"webpack-cli": "^2.1.2",
		"webpack-dev-server": "^3.1.4"
	}
}
