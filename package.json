{
	"name": "tb-editor",
	"version": "1.0.0",
	"description": "一个基于slate.js的所见即所得的富文本编辑器",
	"author": "TB",
	"license": "MIT",
	"private": false,
	"main": "lib/index",
	"files": [
		"lib"
	],
	"keywords": [
		"rich-editor",
		"slate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Durianicecream/TBeditor"
	},
	"scripts": {
		"start": "webpack-dev-server --mode development --history-api-fallback --hot --inline --progress --colors",
		"demo": "rm -rf example/dist/ && webpack --mode production --output-path example/dist/ --pregress --colors",
		"build": "rm -rf lib && mkdir lib && babel src --out-dir lib --copy-files"
	},
	"dependencies": {
		"antd": "^3.16.3",
		"immutable": "^4.0.0-rc.12",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"slate": "^0.44.13",
		"slate-html-serializer": "^0.7.38",
		"slate-react": "^0.21.20",
		"webpack-cli": "^3.3.0"
	},
	"devDependencies": {
		"autoprefixer": "^8.4.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-import": "^1.7.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-runtime": "^6.26.0",
		"css-loader": "^0.28.11",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.0.0",
		"less": "^3.0.4",
		"less-loader": "^4.1.0",
		"mini-css-extract-plugin": "^0.4.0",
		"postcss-loader": "^2.1.5",
		"prettier": "^1.12.1",
		"style-loader": "^0.19.1",
		"url-loader": "~0.6.2",
		"webpack": "^4.8.1",
		"webpack-bundle-analyzer": "^2.11.3",
		"webpack-dev-server": "^3.1.4"
	}
}
